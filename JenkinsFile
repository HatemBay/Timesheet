pipeline {
    agent any

    stages {
		stage('Checkout GIT') {
            steps {
                echo 'Pulling...';
                git branch: 'Raef',
                url: 'https://github.com/HatemBay/Timesheet';
            }
        }

        stage('Build, Testing'){
        	steps {
        		echo 'Building and testing...';
           		bat """mvn clean install"""
        	}    
        }
        
        stage('Test Junit') {
            steps {
                bat """mvn test"""
            }
        }
   
		

		
    }
    
  
}

