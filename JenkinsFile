pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
            //    bat "rmdir  /s /q TicketBookingServiceJunitTesting"
                bat "git clone https://github.com/HatemBay/Timesheet.git"
                bat "mvn clean -f project_pipeline"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f project_pipeline"
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f project_pipeline"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f project_pipeline"
            }
        }
    }
}
