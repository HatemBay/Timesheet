pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
            //    ./ "rmdir  /s /q TicketBookingServiceJunitTesting"
                sh "git clone https://github.com/HatemBay/Timesheet.git"
                sh "mvn clean -f project_pipeline"
            }
        }
        stage('install') {
            steps {
                sh """mvn clean install"""
            }
        }
        stage('test') {
            steps {
                bat"""mvn test"""
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f project_pipeline"
            }
        }
    }
}
